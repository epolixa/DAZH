    sprite_index = spr_amb_frog;
}
else if (!place_meeting(x, y + 1, obj_area_wall) &amp;&amp; !airborn) /* Liftoff */
{
    airborn = true;
    sprite_index = spr_amb_frog_jump;
}

x += xvel; /* Translate */
y += yvel;

if (place_meeting(x, y, obj_area_wall)) /* Fix if stuck in a wall */
{
    x = xprev; xvel = 0;
    y = yprev; yvel = 0;
}
