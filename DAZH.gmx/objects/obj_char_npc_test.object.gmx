            <string>/* Trigger dialogue */
if (place_meeting(x, y, obj_char_player))
{
    if (in_speak &amp;&amp; !speaking)
    {
        speaking = true;
        if (instance_exists(obj_amb_wordbubble))
        {
            with (instance_nearest(x,y,obj_amb_wordbubble)) instance_destroy();
        }
    
        var player = instance_nearest(x, y, obj_char_player);
        player.can_control = false;
        
        var dialogger = instance_create(x, y, obj_char_npc_dialogger);
        dialogger.speaker = id;
        dialogger.DIALOGUE = DIALOGUE;
        dialogger.CHOICES = CHOICES;
    }
    else if (!speaking &amp;&amp; !instance_exists(obj_amb_wordbubble))
    {
        var bub = instance_create(x + sprite_width/2, y - sprite_height*0.8, obj_amb_wordbubble);
        bub.image_index = image_index;
        bub.owner = id;
    }   
}
else if (instance_exists(obj_amb_wordbubble))
{
    var bub = instance_nearest(x, y, obj_amb_wordbubble);
    if (bub.owner = id)
        with (instance_nearest(x,y,obj_amb_wordbubble)) instance_destroy();
}
</string>
