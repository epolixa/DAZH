XACCEL = 0.6; /* Horizontal acceleration */
JUMPSTR = 6;  /* Jump strength */
TERMXVEL = 4; /* Terminal horizontal velocity */
TERMYVEL = 8; /* Terminal verical velocity, should be the same for most objects */

/* Variables */
