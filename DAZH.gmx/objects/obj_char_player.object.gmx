            <kind>1</kind>
            <string>/* Instances */
if (airborn &amp;&amp; ((place_meeting(x + 1, y, obj_area_wall) &amp;&amp; in_right &amp;&amp; !in_left) || (place_meeting(x - 1, y, obj_area_wall) &amp;&amp; in_left &amp;&amp; !in_right))) 
    WALL = instance_place(x + (in_right-in_left)*(sprite_width/2 + 1), y, obj_area_wall);
else if (!airborn) WALL = instance_place(x, y + sprite_height/2 + 1, obj_area_wall);
else WALL = noone;
