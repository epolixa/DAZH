        <arguments>
          <argument>
            <kind>1</kind>
            <string>/* Base checks */
if      (!airborn &amp;&amp; xvel == 0 &amp;&amp; sprite_index != spr_char_player &amp;&amp; sprite_index != spr_char_player_idle) {sprite_index = spr_char_player; image_index = 0; image_speed = 0;} /* Standing */
else if (!airborn &amp;&amp; !in_left &amp;&amp; !in_right &amp;&amp; xvel != 0 &amp;&amp; sprite_index != spr_char_player_slide)          {sprite_index = spr_char_player_slide; image_speed = 0;}          /* Slide (Skid)? */
else if (!airborn &amp;&amp; (in_left || in_right) &amp;&amp; xvel != 0 &amp;&amp; sprite_index != spr_char_player_walk)           {sprite_index = spr_char_player_walk;}                               /* Walking */
