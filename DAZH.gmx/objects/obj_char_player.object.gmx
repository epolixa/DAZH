        <arguments>
          <argument>
            <kind>1</kind>
            <string>/* Movement */
xprev = x; /* Store last valid positions */
yprev = y;

if (!noclip &amp;&amp; place_meeting(x + xvel, y, obj_area_wall)) /* Horz collision */
{
    while (!place_meeting(x + sign(xvel), y, obj_area_wall))
        x += sign(xvel); /* Simulate collision */
    xvel = 0;            /* Halt velocity */      
}

if (!noclip &amp;&amp; place_meeting(x, y + yvel, obj_area_wall)) /* Vert collision */
{
    while (!place_meeting(x, y + sign(yvel), obj_area_wall))
        y += sign(yvel);
    if (sign(yvel) == 1 &amp;&amp; airborn) /* Landing */
    {
        airborn = false; 
        jumps[1] = jumps[0];
        part_particles_create(global.part_sys, x, y + sprite_height/2, player_part_land, 2);
    }
    yvel = 0;      
}
else if (!noclip &amp;&amp; !place_meeting(x, y + 1, obj_area_wall) &amp;&amp; !airborn) /* Liftoff */
{
    airborn = true;
}

x += xvel; /* Translate */
y += yvel;

if (!noclip &amp;&amp; place_meeting(x, y, obj_area_wall)) /* Fix if stuck in a wall */
{
    x = xprev; xvel = 0;
    y = yprev; yvel = 0;
}
</string>
