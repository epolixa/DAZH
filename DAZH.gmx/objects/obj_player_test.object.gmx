          <argument>
            <kind>1</kind>
            <string>/* Horz acceleration */
if (!in_left &amp;&amp; !in_right &amp;&amp; xvel != 0) /* Normal friction */
{
    if (xvel*sign(xvel) &lt; 0.8*XACCEL) xvel = 0;
    else xvel -= (sign(xvel)*(0.8*XACCEL)); 
}
if (in_left &amp;&amp; (-1*xvel) &lt; TERMXVEL) xvel -= XACCEL; /* Left */
if (in_right &amp;&amp; xvel &lt; TERMXVEL)     xvel += XACCEL; /* Right */

/* Vert acceleration */
if (airborne &amp;&amp; yvel &lt; TERMYVEL) yvel += WEIGHT*ENV.GRAVITY; /* Gravity */
if (in_jump) /* Jumping */
{
    if (!airborne || extra_jumps[1] &gt; 0)
        yvel = -1*JUMPSTR;
    if (airborne) 
        extra_jumps[1]--;
}
